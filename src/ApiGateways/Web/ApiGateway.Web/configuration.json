{
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/{version}/posts/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 2
      },
      "SwaggerKey": "Post.Api",

      "DownstreamPathTemplate": "/{version}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "postapi",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/{version}/users/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 2
      },
      "SwaggerKey": "User.Api",

      "DownstreamPathTemplate": "/{version}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "userapi",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/{version}/financial/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put" ],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 2
      },
      "SwaggerKey": "Financial.Api",

      "DownstreamPathTemplate": "/{version}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "financialapi",
          "Port": 80
        }
      ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "Post.Api",
      "Config": [
        {
          "Name": "Post Service API",
          "Version": "v1",
          "Url": "http://postapi:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "User.Api",
      "Config": [
        {
          "Name": "User Service API",
          "Version": "v1",
          "Url": "http://userapi:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "Financial.Api",
      "Config": [
        {
          "Name": "Financial Service API",
          "Version": "v1",
          "Url": "http://financialapi:80/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost",
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Request rate limit exceded",
      "HttpStatusCode": 429
    }
  }
}